{
    "name": "intersoccer/player-management",
    "description": "InterSoccer Player Management Plugin",
    "require": {
        "phpoffice/phpspreadsheet": "^1.29",
        "dompdf/dompdf": "^2.0",
        "deliciousbrains/wp-background-processing": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "mockery/mockery": "^1.4",
        "10up/wp_mock": "^0.5.0"
    },
    "autoload": {
        "psr-4": {
            "InterSoccer\\": "includes/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "test:unit": "phpunit --testsuite=unit",
        "test:integration": "phpunit --testsuite=integration",
        "test:coverage": "phpunit --coverage-html coverage/",
        "test:coverage-text": "phpunit --coverage-text"
    }
}
